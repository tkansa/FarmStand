@model ProduceListViewModel

<h1>@Model.TableName</h1>
@foreach (Produce item in Model.Produce)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="caption">
            <h3 class="pull-right">@item.Price.ToString("c")</h3>
            <h3>
                <a>@item.Name</a>
            </h3>
            <h3>@item.Category.CategoryName</h3>
        </div>
    </div>
}
